# Environment variables for Imran Khan Vote App
# Copy this file to .env and fill in the values

# Vite environment variables (prefixed with VITE_)
VITE_APP_NAME="Imran Khan Vote"
VITE_APP_DESCRIPTION="Vote for Imran Khan"
VITE_APP_VERSION=1.0.0

# Contract configuration
VITE_CONTRACT_ADDRESS=0x52d3778ffbB1024b44D55214b3385e1e0F7A1354
VITE_CHAIN_ID=1  # Ethereum Mainnet

# API configuration (if any)
# VITE_API_URL=https://api.example.com

# WalletConnect Project ID (if using WalletConnect)
# VITE_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id
